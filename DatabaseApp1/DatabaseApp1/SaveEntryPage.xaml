<Page x:Class="DatabaseApp1.SaveEntryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DatabaseApp1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Save Entry">

    <Grid Background="#FFB9B9B9">
        <Label x:Name="SaveLabel" Content="Entry saved successfully!" HorizontalAlignment="Center" VerticalAlignment="Top" Height="34" Width="276" Margin="0,200,0,0" FontSize="20" FontFamily="Verdana"/>

        <Button x:Name="SaveEntryBackButton" Content="Back" HorizontalAlignment="Left" Margin="546,367,0,0" VerticalAlignment="Top" Width="151" Height="32" Click="SaveEntryBackButton_Click"/>
    </Grid>
</Page>
